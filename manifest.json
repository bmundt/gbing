{
  "manifest_version" : 2,
  "name" : "gbing",
  "description" : "Searches bing when you search google",
  "version" : "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background" : { "scripts" : [ "background.js"]},
  "icons" : { "16" : "icon.png" },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.google.com/*",
    "*://*.bing.com/*"
  ]
}
